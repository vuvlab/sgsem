ans= sgSEMp1(acrylic1)
library(sgSEM)
library(segmented)
x <- c(1:150, 151:300)
y <- numeric(300)
## Create first segment
y[1:150] <- rep(150,150) + rnorm(150, 1, 7)
## Create second segment
y[151:300] <- 150:1 + rnorm(150, 2, 12)
plot(x,y)
O= segmented.lm(lm(y~x),seg.Z = ~x, psi=list(x=2.61), control = seg.control(stop.if.error = FALSE,n.boot=0, it.max=20))
## this command will automatically estimate the breakpoint (here I wrote x=2.61, but it estimated a different point)
summary(O)
plot(O, main="Linear Change Point", xlab="Time", ylab="Pmax", col=4,lwd=4)
points(x,y, col=2)
plot(O, main="Linear Change Point", xlim= c(0, 320), y=c(0, 180) xlab="Time", ylab="Pmax", col=4,lwd=4)
plot(O, main="Linear Change Point", xlim= c(0, 320), ylim=c(0, 180), xlab="Time", ylab="Pmax", col=4,lwd=4)
points(x,y, col=2)
slope(o)
slope(O)
CHP = data.frame(x,y)
setwd("C:/Users/mohamed/Desktop/changpointDF")
write.csv(CHP, file="CHPdata.csv")
